{
  "version": 2,
  "builds": [
    {
      "src": "api/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["db.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "api/(.*)",
      "dest": "/api/index.js"
      

    },
    {
      "src": "/(.*)",
      "dest": "/$1" 
    }
  ]
}